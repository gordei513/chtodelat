import math
k = 25
def prosto(k):
    if k > 1:
        for i in range(2, math.ceil(math.sqrt(k))+1):
             if k % i == 0:
                return 0
                break
        else:
            return 1

    else:
        return 1
prosto(k)
print (prosto(k))
print (prosto(17))
print (prosto(25))
print (prosto(7))
